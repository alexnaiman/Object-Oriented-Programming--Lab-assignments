{
  "presets": [
    ["es2015", { "modules": false }],
    "react",
    "airbnb",
    "stage-0",
    "stage-2"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-runtime", {
          "polyfill": false,
          "regenerator": true
        },
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        "istanbul"
      ]
    }
  }
}
